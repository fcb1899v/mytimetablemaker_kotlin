                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:text="@string/xxxstation"
                            android:textColor="@color/colorWhite"
                            android:textSize="20sp"
                            tools:ignore="NestedWeights,UnusedAttribute" />

                        <TextView
